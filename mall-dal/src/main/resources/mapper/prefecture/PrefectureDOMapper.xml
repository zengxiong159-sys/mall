    s.SKU_PIC_URL as sku_SKU_PIC_URL,
    s.PRODUCT_LOCK_STOCK as sku_PRODUCT_LOCK_STOCK,
    s.PRODUCT_SP_DATA as sku_PRODUCT_SP_DATA
    from
    CMS_PREFECTURE c
    left join CMS_PREFECTURE_STOCK_RELATION r on c.id = r.PREFECTURE_ID
    left join pms_product p on p.product_id = r.PRODUCT_ID
    left join pms_sku_stock s on p.product_id = s.product_id
    where
            p.PUBLISH_STATUS = 2
    <if test="id != null and id != ''">
      and c.ID =#{id,jdbcType=DECIMAL}
    </if>
    <if test="prefectureName != null and prefectureName != ''">
      and c.PREFECTURE_NAME like concat('%',#{prefectureName,jdbcType=VARCHAR},'%')
