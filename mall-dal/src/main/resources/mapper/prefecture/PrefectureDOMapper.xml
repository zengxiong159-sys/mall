  <select id="selectPrefectureProductInfoByParams" resultMap="PrefectureProductInfoResultMap" parameterType="com.qdbank.mall.model.prefecture.PrefectureDO">
    select
    r.product_level as pro_product_level,
    r.PRODUCT_ID as pro_PRODUCT_ID,
    p.MERCHANT_NO as pro_MERCHANT_NO,
    p.MERCHANT_NAME as pro_MERCHANT_NAME,
    p.MAIL_PIC_URL as pro_MAIL_PIC_URL,
    p.CATEGORY_ID_1 as pro_CATEGORY_ID_1,
    p.CATEGORY_ID_2 as pro_CATEGORY_ID_2,
    p.CATEGORY_ID_3 as pro_CATEGORY_ID_3,
    p.CATEGORY_ID_4 as pro_CATEGORY_ID_4,
    p.PRODUCT_NAME as pro_PRODUCT_NAME,
    <include refid="com.qdbank.mall.mapper.product.ProductDOMapper.Blob_Column_List"/>,
    p.PUBLISH_STATUS as pro_PUBLISH_STATUS,
    p.UPDATE_TIME as pro_UPDATE_TIME
    from
    CMS_PREFECTURE_STOCK_RELATION r
    left join pms_product p on p.product_id = r.PRODUCT_ID
    where
    p.PUBLISH_STATUS = 2
    <if test="id != null and id != ''">
      and r.PREFECTURE_ID =#{id,jdbcType=DECIMAL}
    </if>
    order by  pro_product_level asc , pro_UPDATE_TIME desc
  </select>

  <update id="updateStatusByPrimaryKey" parameterType="com.qdbank.mall.model.prefecture.PrefectureDO">
    update CMS_PREFECTURE
    <set>
      <if test="status != null">
        STATUS = #{status,jdbcType=DECIMAL},
      </if>
      <if test="createTime != null">
        CREATE_TIME = #{createTime,jdbcType=TIMESTAMP},
      </if>
      <if test="updateTime != null">
        UPDATE_TIME = #{updateTime,jdbcType=TIMESTAMP},
      </if>
      <if test="createdBy != null">
        CREATED_BY = #{createdBy,jdbcType=VARCHAR},
      </if>
      <if test="updatedBy != null">
        UPDATED_BY = #{updatedBy,jdbcType=VARCHAR},
      </if>
    </set>
    where ID = #{id,jdbcType=DECIMAL}
  </update>
  <update id="updateExpireStatus">
   update
        CMS_PREFECTURE
   set
        STATUS = 0,
        UPDATE_TIME = sysdate,
        UPDATED_BY = 'SYSTEM'
    where
         END_TIME &lt; sysdate
    and
         STATUS = 1
  </update>
