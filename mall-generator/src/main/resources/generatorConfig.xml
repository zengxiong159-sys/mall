<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <properties resource="generator.properties"/>
    <context id="MySqlContext" targetRuntime="MyBatis3" defaultModelType="flat">
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>
        <property name="javaFileEncoding" value="UTF-8"/>
        <property name="remarksReporting" value="true"></property>
        <!-- 为模型生成序列化方法-->
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
        <!-- 为生成的Java模型创建一个toString方法 -->
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin"/>
        <!--生成mapper.xml时覆盖原文件-->
        <plugin type="org.mybatis.generator.plugins.UnmergeableXmlMappersPlugin" />
        <commentGenerator type="com.qdbank.generator.CommentGenerator">
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
            <property name="suppressDate" value="true"/>
            <property name="addRemarkComments" value="true"/>
        </commentGenerator>

<!--        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver" connectionURL="jdbc:mysql://10.238.146.162:3306/spglog?serverTimezone=UTC" userId="bigdata" password="Cqmyg#1235">-->
<!--            <property name="remarksReporting" value="true"/> &lt;!&ndash;oracle配置实体类注释&ndash;&gt;-->
<!--        </jdbcConnection>-->

        <jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver" connectionURL="jdbc:oracle:thin:@10.238.148.92:1521/mall" userId="malldata" password="VNK#UC#8h3">
            <property name="remarksReporting" value="true"/> <!--oracle配置实体类注释-->
        </jdbcConnection>
        <javaTypeResolver type="com.qdbank.generator.MyJavaType">
            <property name="forceBigDecimals" value="false"></property>
        </javaTypeResolver>
        <javaModelGenerator targetPackage="com.qdbank.mall.model.umsuser" targetProject="mall-generator\src\main\java"/>

        <sqlMapGenerator targetPackage="mapper.umsuser" targetProject="mall-generator\src\main\resources"/>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.qdbank.mall.mapper.umsuser"
                             targetProject="mall-generator\src\main\java"/>
        <!--生成全部表tableName设为%-->
        <table  tableName="ums_user_info" domainObjectName="UmsUserInfoDO" enableCountByExample="true" enableUpdateByExample="true" enableDeleteByExample="true" enableSelectByExample="true" selectByExampleQueryId="true">
            <!--            <columnOverride column="create_time" jdbcType="TIMESTAMP" />-->
        </table>
    </context>
</generatorConfiguration>